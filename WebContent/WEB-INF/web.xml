<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>Form</display-name>
  <welcome-file-list>
    <welcome-file>/Welcome.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>LoginRegister</servlet-name>
    <servlet-class>com.ait.vivek.LoginRegister</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
    <servlet-name>DisplayALLUser</servlet-name>
    <servlet-class>com.ait.vivek.DisplayALLUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DeleteByName</servlet-name>
    <servlet-class>com.ait.vivek.DeleteByName</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>SupportJSP</servlet-name>
    <servlet-class>com.ait.vivek.SupportJSP</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>AddUser</servlet-name>
    <servlet-class>com.ait.vivek.AddUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DHTMLxServlet</servlet-name>
    <servlet-class>com.ait.vivek.DHTMLxServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ContextMenuServlet</servlet-name>
    <servlet-class>com.ait.vivek.ContextMenuServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>TabbarServlet</servlet-name>
    <servlet-class>com.ait.vivek.TabbarServlet</servlet-class>
    <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginRegister</servlet-name>
    <url-pattern>/LoginRegister</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DisplayALLUser</servlet-name>
    <url-pattern>/DisplayALLUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DeleteByName</servlet-name>
    <url-pattern>/DeleteByName</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SupportJSP</servlet-name>
    <url-pattern>/SupportJSP</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>AddUser</servlet-name>
    <url-pattern>/AddUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DHTMLxServlet</servlet-name>
    <url-pattern>/DHTMLxServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ContextMenuServlet</servlet-name>
    <url-pattern>/ContextMenuServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TabbarServlet</servlet-name>
    <url-pattern>/TabbarServlet</url-pattern>
  </servlet-mapping>
  <filter>
    <display-name>ValidateFilter</display-name>
    <filter-name>ValidateFilter</filter-name>
    <filter-class>com.ait.vivek.ValidateFilter</filter-class>
    <init-param>
      <param-name>Key</param-name>
      <param-value>First-Filter</param-value>
    </init-param>
  </filter>
  <filter-mapping>
    <filter-name>ValidateFilter</filter-name>
    <url-pattern>/LoginRegister</url-pattern>
  </filter-mapping>
</web-app>